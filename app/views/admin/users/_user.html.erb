<tr>
  <td><%= user.id %></td>
  <td><%= link_to user.name, [:edit, :admin, user] %></td>
  <td><%= user.email %></td>
  <td><%= link_to "Delete", [:admin, user], method: :delete,
                            data: {confirm: "Are you sure?"},
                            class: "btn btn-danger" %></td>
</tr>
